{
  "name": "prompt-vault",
  "displayName": "prompt-vault",
  "description": "Save and reuse prompts inside VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:promptVault.hello"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "promptVault.hello",
        "title": "Prompt Vault: Hello"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/node": "22.x",
    "typescript": "^5.9.3"
  }
}
